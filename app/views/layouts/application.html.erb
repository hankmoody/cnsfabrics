  <meta name="google-site-verification" content="sAJeI1mPFk0TXbIQi9HuQft7vBXblcCI-U1DdPdDyPc" />
  <title><%=site_title%></title>
  <%= yield :head %>
  <%= stylesheet_link_tag    "application", media: "all", "data-turbolinks-track" => true %>
